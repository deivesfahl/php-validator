{
    "name": "deivesfahl/validator",
    "description": "Biblioteca para validação de dados",
    "type": "library",
    "authors": [
        {
            "name": "Deives Fahl",
            "email": "dfahl.cps@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v3.9.5",
        "pestphp/pest": "v1.21.3",
        "symfony/var-dumper": "v6.0.11"
    },
    "autoload": {
        "psr-4": {
            "Validator\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    }
}
